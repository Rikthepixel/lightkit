{
  "name": "@minkit/fetch",
  "description": "A lightweight fetch wrapper",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "test": "jest --passWithNoTests --coverage --silent=false"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "fastify": "^4.19.2",
    "jest": "^29.6.1",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "fetch",
    "wrapper",
    "light"
  ],
  "author": "Rikthepixel",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rikthepixel/minkit.git"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/Rikthepixel/minkit/issues"
  },
  "homepage": "https://github.com/Rikthepixel/minkit/blob/main/packages/fetch/README.md",
  "gitHead": "06418545a1ffb419c44231684ab959f255cca0fc",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    "./register": {
      "import": "./lib/modules/register.mjs",
      "require": "./lib/modules/register.js",
      "types": "./lib/modules/register.d.ts"
    },
    "./response": {
      "import": "./lib/modules/response.mjs",
      "require": "./lib/modules/response.js",
      "types": "./lib/modules/response.d.ts"
    },
    "./request": {
      "import": "./lib/modules/request.mjs",
      "require": "./lib/modules/request.js",
      "types": "./lib/modules/request.d.ts"
    },
    "./errors": {
      "import": "./lib/errors.mjs",
      "require": "./lib/errors.js",
      "types": "./lib/errors.d.ts"
    },
    "./types.ts": {
      "import": "./lib/types.mjs",
      "require": "./lib/types.js",
      "types": "./lib/types.d.ts"
    }
  }
}
